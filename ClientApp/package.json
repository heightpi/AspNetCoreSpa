  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "clean:install": "rimraf node_modules & npm i",
    "start": "ng serve --extract-css --aot",
    "build": "ng build --extract-css --aot",
    "build:ssr": "npm run build -- --app=ssr --output-hashing=media",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "bundle:analyze": "npm run build -- --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
